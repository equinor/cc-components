{
  "name": "@equinor/workspace-ag-grid",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "sideEffects": false,
  "license": "MIT",
  "scripts": {
    "build": "tsup ./src/index.ts ./src/modules/index.ts --format cjs,esm --dts --clean",
    "build:prod": "tsup ./src/index.ts ./src/modules/index.ts --format cjs,esm --dts --clean"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./modules": {
      "require": "./dist/modules/index.cjs",
      "import": "./dist/modules/index.js",
      "types": "./dist/modules/index.d.ts"
    }
  },
  "dependencies": {
    "@equinor/eds-tokens": "^0.9.2",
    "@equinor/fusion-framework-react-ag-grid": "33.2.8",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "peerDependencies": {
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@types/jest": "30.0.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/styled-components": "^5.1.34",
    "styled-components": "^6.1.19",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  }
}
