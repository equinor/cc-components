{
  "name": "@equinor/workspace-fusion",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "sideEffects": false,
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "build:prod": "tsup"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "typesVersions": {
    ">=4.2": {
      ".": [
        "dist/types/index"
      ],
      "garden": [
        "dist/types/lib/integrations/garden/index"
      ],
      "grid": [
        "dist/types/lib/integrations/grid/index"
