{
  "name": "cc-components",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@9.15.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "first-time-setup": "npm i -g vite pnpm@9.15.0 @equinor/fusion-framework-cli turbo && pnpm i && pnpm ci:build",
    "serve": "turbo run dev --filter",
    "serve:local": "turbo run dev:local --filter",
    "ci:build": "turbo run build --output-logs errors-only --concurrency 1",
    "build": "turbo run build --concurrency 15",
    "build:app": "turbo run build --concurrency 15 --filter",
    "build:changed": "turbo run build --since main --scope * --concurrency 15",
    "build:reports": "turbo run build --filter ./reports/** --concurrency 15",
    "build:apps": "turbo run build --filter ./apps/** --concurrency 15",
    "build:libs": "turbo run build --filter ./libs/* --concurrency 15",
    "build:portal": "turbo run build --filter './portal/*'",
    "build:fusion-portal": "turbo  build --filter './fusion-portal/**'",
    "new:app": "nx generate @cc-components/plugins:fusion-app-generator",
    "new:report": "nx generate @cc-components/plugins:fusion-report-generator",
    "watch": "node ./run_watch.js dev",
    "watch:local": "node ./run_watch.js dev:local",
    "clean": "pnpm clean:build && pnpm clean:npm",
    "clean:npm": "pnpm -r exec rimraf node_modules && pnpm exec rimraf node_modules",
    "clean:build": "pnpm -r exec rimraf dist tmp .cache .turbo tsconfig.tsbuildinfo",
    "bump-deps": "pnpm up --latest --recursive --interactive"
  },
  "devDependencies": {
    "@equinor/fusion-framework-cli": "catalog:",
    "@nrwl/devkit": "catalog:",
    "@nrwl/js": "catalog:",
    "@swc-node/register": "catalog:",
    "@swc/core": "catalog:",
    "@testing-library/jest-dom": "catalog:",
    "@testing-library/react": "catalog:",
    "@testing-library/react-hooks": "catalog:",
    "@types/html-escaper": "catalog:",
    "@types/luxon": "catalog:",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "cross-env": "catalog:",
    "eslint-plugin-import": "catalog:",
    "eslint-plugin-jsx-a11y": "catalog:",
    "eslint-plugin-react": "catalog:",
    "eslint-plugin-react-hooks": "catalog:",
    "eslint-plugin-tsdoc": "catalog:",
    "nx": "catalog:",
    "rollup-plugin-inject-process-env": "catalog:",
    "ts-node": "catalog:",
    "tsx": "catalog:",
    "turbo": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-plugin-environment": "catalog:",
    "vite-plugin-eslint": "catalog:",
    "vite-tsconfig-paths": "catalog:",
    "vitest": "catalog:"
  },
  "dependencies": {
    "@equinor/eds-core-react": "catalog:",
    "@equinor/eds-icons": "catalog:",
    "@equinor/eds-tokens": "catalog:",
    "@equinor/fusion-framework-module": "catalog:",
    "@equinor/fusion-framework-module-ag-grid": "catalog:",
    "@equinor/fusion-framework-module-event": "catalog:",
    "@equinor/fusion-framework-module-http": "catalog:",
    "@equinor/fusion-framework-module-msal": "catalog:",
    "@equinor/fusion-framework-react": "catalog:",
    "@equinor/fusion-framework-react-app": "catalog:",
    "@equinor/fusion-framework-react-module-bookmark": "catalog:",
    "@equinor/fusion-framework-react-module-context": "catalog:",
    "@equinor/fusion-framework-react-module-http": "catalog:",
    "@equinor/fusion-observable": "catalog:",
    "@equinor/workspace-fusion": "workspace:*",
    "@microsoft/applicationinsights-web": "catalog:",
    "@swc/helpers": "catalog:",
    "@tanstack/react-query": "catalog:",
    "core-js": "catalog:",
    "luxon": "catalog:",
    "odata-query": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-error-boundary": "catalog:",
    "rimraf": "catalog:",
    "rxjs": "catalog:",
    "styled-components": "catalog:",
    "stylis": "catalog:"
  },
  "engines": {
    "node": "20"
  },
  "pnpm": {
    "overrides": {
      "react": "^19.1.1",
      "react-dom": "^19.1.1"
    },
    "peerDependencyRules": {
      "allowedVersions": {
        "react": "^19.1.1",
        "react-dom": "^19.1.1"
      },
      "ignoreMissing": [
        "react",
        "react-dom",
        "@babel/*",
        "@types/*"
      ]
    }
  },
  "overrides": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "@types/react": "19.1.9"
  },
  "resolutions": {
    "styled-components": "6.1.19"
  }
}
