{
  "name": "cc-components",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@8.0.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "first-time-setup": "npm i -g vite pnpm@8.0.0 @equinor/fusion-framework-cli turbo && pnpm i && pnpm ci:build",
    "serve": "turbo run dev --filter",
    "ci:build": "turbo run build --output-logs errors-only --concurrency 1",
    "build": "turbo run build --output-logs errors-only --parallel",
    "build:app": "turbo run build --parallel --output-logs errors-only --filter",
    "build:changed": "turbo run build --since main --parallel --output-logs errors-only",
    "build:reports": "turbo run build --filter ./reports/** --parallel --output-logs errors-only",
    "build:apps": "turbo run build --filter ./apps/** --parallel --output-logs errors-only",
    "build:libs": "turbo run build --filter ./libs/* --parallel --output-logs errors-only",
    "new:app": "nx generate @cc-components/plugins:fusion-app-generator",
    "new:report": "nx generate @cc-components/plugins:fusion-report-generator",
    "shipit:app": "turbo run shipit --filter",
    "shipit:reports": "turbo run shipit --filter ./reports/**"
  },
  "devDependencies": {
    "@equinor/fusion-framework-cli": "^5.1.16",
    "@nrwl/devkit": "15.7.1",
    "@swc-node/register": "1.4.2",
    "@swc/core": "1.3.35",
    "@types/html-escaper": "^3.0.0",
    "@types/luxon": "^3.0.1",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "@types/react-is": "17.0.3",
    "@types/styled-components": "5.1.26",
    "cross-env": "^7.0.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "nx": "15.7.1",
    "rollup-plugin-inject-process-env": "^1.3.1",
    "turbo": "^1.9.1",
    "typescript": "~5.0.4",
    "vite": "^4.0.5",
    "vite-plugin-environment": "^1.1.3",
    "vite-tsconfig-paths": "^4.0.2"
  },
  "dependencies": {
    "@equinor/eds-core-react": "^0.28.0",
    "@equinor/eds-icons": "^0.17.0",
    "@equinor/eds-tokens": "^0.9.0",
    "@equinor/fusion-framework-module-ag-grid": "^4.0.0",
    "@equinor/fusion-framework-react-app": "^3.0.9",
    "@equinor/fusion-framework-react-module-context": "^4.0.1",
    "@equinor/workspace-fusion": "5.0.6",
    "@equinor/workspace-sidesheet": "^0.1.6",
    "@microsoft/applicationinsights-web": "^3.0.2",
    "@swc/helpers": "0.4.14",
    "@tanstack/react-query": "^4.23.0",
    "core-js": "^3.6.5",
    "luxon": "^3.2.1",
    "odata-query": "^7.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-is": "18.2.0",
    "rxjs": "^7.5.7",
    "styled-components": "5.3.6"
  }
}
