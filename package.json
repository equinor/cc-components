{
  "name": "cc-components",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@8.0.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "first-time-setup": "npm i -g vite pnpm@8.0.0 @equinor/fusion-framework-cli turbo && pnpm i && pnpm ci:build",
    "serve": "turbo run dev --filter",
    "serve:local": "turbo run dev:local --filter",
    "ci:build": "turbo run build --output-logs errors-only --concurrency 1",
    "build": "turbo run build --output-logs errors-only --parallel",
    "build:app": "turbo run build --parallel --output-logs errors-only --filter",
    "build:changed": "turbo run build --since main --parallel --output-logs errors-only",
    "build:reports": "turbo run build --filter ./reports/** --parallel --output-logs errors-only",
    "build:apps": "turbo run build --filter ./apps/** --parallel --output-logs errors-only",
    "build:libs": "turbo run build --filter ./libs/* --parallel --output-logs errors-only",
    "new:app": "nx generate @cc-components/plugins:fusion-app-generator",
    "new:report": "nx generate @cc-components/plugins:fusion-report-generator"
  },
  "devDependencies": {
    "@equinor/fusion-framework-cli": "^9.3.5",
    "@nrwl/devkit": "16.9.1",
    "@nrwl/js": "^17.0.2",
    "@swc-node/register": "1.6.7",
    "@swc/core": "1.3.89",
    "@testing-library/jest-dom": "^6.1.2",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/html-escaper": "^3.0.0",
    "@types/luxon": "^3.3.2",
    "@types/node": "20.7.0",
    "@types/react": "18.2.22",
    "@types/react-dom": "18.2.7",
    "@types/react-is": "18.2.1",
    "@types/styled-components": "5.1.28",
    "cross-env": "^7.0.3",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "nx": "15.7.1",
    "rollup-plugin-inject-process-env": "^1.3.1",
    "turbo": "^1.10.14",
    "typescript": "~5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-environment": "^1.1.3",
    "vite-plugin-eslint": "^1.8.1",
    "vite-tsconfig-paths": "^4.0.2",
    "vitest": "^0.25.8"
  },
  "dependencies": {
    "@equinor/eds-core-react": "^0.28.0",
    "@equinor/eds-icons": "0.19.3",
    "@equinor/eds-tokens": "0.9.2",
    "@equinor/fusion-framework-module-ag-grid": "^30.1.1",
    "@equinor/fusion-framework-react-app": "^4.1.8",
    "@equinor/fusion-framework-react-module-context": "^6.0.14",
    "@equinor/workspace-fusion": "5.4.1",
    "@equinor/workspace-sidesheet": "^0.1.6",
    "@microsoft/applicationinsights-web": "^3.0.3",
    "@swc/helpers": "0.5.2",
    "@tanstack/react-query": "^4.35.3",
    "core-js": "^3.32.2",
    "luxon": "^3.4.3",
    "odata-query": "^7.0.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-is": "18.2.0",
    "rxjs": "^7.8.1",
    "styled-components": "5.3.6"
  }
}
