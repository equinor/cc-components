{
  "name": "cc-components",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@9.0.5",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "first-time-setup": "npm i -g vite pnpm@9.0.5 @equinor/fusion-framework-cli turbo && pnpm i && pnpm ci:build",
    "serve": "turbo run dev --filter",
    "serve:local": "turbo run dev:local --filter",
    "ci:build": "turbo run build --output-logs errors-only --concurrency 1",
    "build": "turbo run build --output-logs errors-only --parallel",
    "build:app": "turbo run build --parallel --output-logs errors-only --filter",
    "build:changed": "turbo run build --since main --scope * --parallel --output-logs errors-only",
    "build:reports": "turbo run build --filter ./reports/** --parallel --output-logs errors-only",
    "build:apps": "turbo run build --filter ./apps/** --parallel --output-logs errors-only",
    "build:libs": "turbo run build --filter ./libs/* --parallel --output-logs errors-only",
    "build:portal": "turbo run build --filter './portal/*' --output-logs errors-only",
    "build:fusion-portal": "turbo  build --filter './fusion-portal/**' --output-logs errors-only",
    "new:app": "nx generate @cc-components/plugins:fusion-app-generator",
    "new:report": "nx generate @cc-components/plugins:fusion-report-generator",
    "watch": "node ./run_watch.js dev",
    "watch:local": "node ./run_watch.js dev:local",
    "link:workspace": "tsx ./github-action/src/link-ff.ts link",
    "unlink:workspace": "tsx ./github-action/src/link-ff.ts unlink",
    "clean": "pnpm clean:build && pnpm clean:npm",
    "clean:npm": "pnpm -r exec rimraf node_modules && pnpm exec rimraf node_modules",
    "clean:build": "pnpm -r exec rimraf dist tmp .cache .turbo tsconfig.tsbuildinfo",
    "bump-deps": "pnpm up --latest --recursive --interactive"
  },
  "devDependencies": {
    "@equinor/fusion-framework-cli": "^10.7.5",
    "@nrwl/devkit": "19.8.4",
    "@nrwl/js": "^19.8.4",
    "@swc-node/register": "1.10.10",
    "@swc/core": "1.13.3",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/html-escaper": "^3.0.4",
    "@types/luxon": "^3.7.0",
    "@types/node": "24.1.0",
    "@types/react": "19.1.9",
    "@types/react-dom": "19.1.7",
    "cross-env": "^10.0.0",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-tsdoc": "^0.4.0",
    "nx": "21.3.8",
    "rollup-plugin-inject-process-env": "^1.3.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "turbo": "^2.5.5",
    "typescript": "5.8.3",
    "vite": "^7.0.6",
    "vite-plugin-environment": "^1.1.3",
    "vite-plugin-eslint": "^1.8.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@equinor/eds-core-react": "0.47.0",
    "@equinor/eds-icons": "0.22.0",
    "@equinor/eds-tokens": "0.9.2",
    "@equinor/fusion-framework-module": "4.4.2",
    "@equinor/fusion-framework-module-ag-grid": "34.0.2",
    "@equinor/fusion-framework-module-event": "4.3.6",
    "@equinor/fusion-framework-module-http": "6.3.3",
    "@equinor/fusion-framework-module-msal": "4.0.7",
    "@equinor/fusion-framework-react": "7.4.15",
    "@equinor/fusion-framework-react-app": "6.2.4",
    "@equinor/fusion-framework-react-module-bookmark": "4.0.11",
    "@equinor/fusion-framework-react-module-context": "6.2.31",
    "@equinor/fusion-framework-react-module-http": "9.0.3",
    "@equinor/fusion-observable": "8.5.1",
    "@equinor/workspace-fusion": "link:workspace/packages/workspace-fusion",
    "@microsoft/applicationinsights-web": "3.3.9",
    "@swc/helpers": "0.5.17",
    "@tanstack/react-query": "^5.83.0",
    "core-js": "^3.44.0",
    "luxon": "^3.7.1",
    "odata-query": "^8.0.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-error-boundary": "^6.0.0",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.2",
    "styled-components": "^6.1.19",
    "stylis": "^4.3.6"
  },
  "engines": {
    "node": "20"
  },
  "pnpm": {
    "overrides": {
      "react": "^19.1.1",
      "react-dom": "^19.1.1"
    },
    "peerDependencyRules": {
      "allowedVersions": {
        "react": "^19.1.1",
        "react-dom": "^19.1.1"
      },
      "ignoreMissing": [
        "react",
        "react-dom",
        "@babel/*",
        "@types/*"
      ]
    }
  },
  "overrides": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "styled-components": "^6.1.19",
    "@types/react": "19.1.9"
  }
}
