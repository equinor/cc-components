{
  "name": "cc-components",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@9.0.5",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "first-time-setup": "npm i -g vite pnpm@9.0.5 @equinor/fusion-framework-cli turbo && pnpm i && pnpm ci:build",
    "serve": "turbo run dev --filter",
    "serve:local": "turbo run dev:local --filter",
    "ci:build": "turbo run build --output-logs errors-only --concurrency 1",
    "build": "turbo run build --output-logs errors-only --parallel",
    "build:app": "turbo run build --parallel --output-logs errors-only --filter",
    "build:changed": "turbo run build --since main --scope * --parallel --output-logs errors-only",
    "build:reports": "turbo run build --filter ./reports/** --parallel --output-logs errors-only",
    "build:apps": "turbo run build --filter ./apps/** --parallel --output-logs errors-only",
    "build:libs": "turbo run build --filter ./libs/* --parallel --output-logs errors-only",
    "build:portal": "turbo run build --filter './portal/*' --output-logs errors-only",
    "build:fusion-portal": "turbo  build --filter './fusion-portal/**' --output-logs errors-only",
    "new:app": "nx generate @cc-components/plugins:fusion-app-generator",
    "new:report": "nx generate @cc-components/plugins:fusion-report-generator",
    "watch": "node ./run_watch.js dev",
    "watch:local": "node ./run_watch.js dev:local",
    "link:workspace": "tsx ./github-action/src/link-ff.ts link",
    "unlink:workspace": "tsx ./github-action/src/link-ff.ts unlink",
    "bump-deps": "pnpm up --latest --recursive --interactive"
  },
  "devDependencies": {
    "@equinor/fusion-framework-cli": "^10.0.2",
    "@nrwl/devkit": "18.3.4",
    "@nrwl/js": "^18.3.5",
    "@swc-node/register": "1.9.0",
    "@swc/core": "1.5.6",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^15.0.7",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/html-escaper": "^3.0.4",
    "@types/luxon": "^3.6.2",
    "@types/node": "20.12.7",
    "@types/react": "18.3.2",
    "@types/react-dom": "18.3.0",
    "@types/react-is": "18.2.1",
    "cross-env": "^7.0.3",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-react-hooks": "4.6.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "nx": "18.3.4",
    "rollup-plugin-inject-process-env": "^1.3.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.2",
    "turbo": "^1.13.4",
    "typescript": "5.0.2",
    "vite": "^5.4.19",
    "vite-plugin-environment": "^1.1.3",
    "vite-plugin-eslint": "^1.8.1",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.1"
  },
  "dependencies": {
    "@equinor/eds-core-react": "^0.37.0",
    "@equinor/eds-icons": "0.21.0",
    "@equinor/eds-tokens": "0.9.2",
    "@equinor/fusion-framework-module": "^4.3.5",
    "@equinor/fusion-framework-module-ag-grid": "34.0.2",
    "@equinor/fusion-framework-module-event": "^4.2.4",
    "@equinor/fusion-framework-module-http": "^6.2.0",
    "@equinor/fusion-framework-module-msal": "^3.1.5",
    "@equinor/fusion-framework-react": "7.3.0",
    "@equinor/fusion-framework-react-app": "5.2.10",
    "@equinor/fusion-framework-react-module-bookmark": "^2.1.18",
    "@equinor/fusion-framework-react-module-context": "^6.2.13",
    "@equinor/fusion-framework-react-module-http": "^8.0.0",
    "@equinor/fusion-observable": "8.4.1",
    "@equinor/workspace-fusion": "9.0.37-pr.3",
    "@microsoft/applicationinsights-web": "3.3.6",
    "@swc/helpers": "0.5.12",
    "@tanstack/react-query": "^5.80.7",
    "core-js": "^3.43.0",
    "luxon": "^3.6.1",
    "odata-query": "^7.0.9",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-error-boundary": "^4.1.2",
    "react-is": "18.3.1",
    "rxjs": "^7.8.2",
    "styled-components": "6.1.12",
    "stylis": "^4.3.6"
  },
  "engines": {
    "node": "20"
  },
  "pnpm": {
    "overrides": {
      "react-dom": "18.3.1",
      "react": "18.3.1"
    },
    "peerDependencyRules": {
      "allowedVersions": {
        "react-dom": "18.3.1",
        "react": "18.3.1"
      },
      "ignoreMissing": [
        "react",
        "react-dom",
        "@babel/*",
        "@types/*"
      ]
    }
  }
}
