{
  "name": "jcainstallation",
  "version": "0.0.2",
  "main": "src/main.tsx",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "fusion-framework-cli app dev",
    "build": "tsc --noEmit",
    "build:spa": "fusion-framework-cli app build",
    "postbuild:spa": "zip dist/bundle.zip -Dj dist/app-bundle.js app-manifest.json",
    "deploy:spa": "fdev portal upload -e ci -k jca-installation dist/bundle.zip",
    "postdeploy:spa": "fdev portal publish -e ci -k jca-installation"
  }
}
