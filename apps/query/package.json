{
  "name": "query",
  "version": "0.0.3",
  "main": "src/main.tsx",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "fusion-framework-cli app dev",
    "build": "tsc --noEmit",
    "build:spa": "fusion-framework-cli app build",
    "postbuild:spa": "zip dist/bundle.zip -Dj dist/app-bundle.js app-manifest.json",
    "deploy:spa": "fdev portal upload -e ci -k query-workspace dist/bundle.zip",
    "postdeploy:spa": "fdev portal publish -e ci -k query-workspace",
    "patch": "npm version patch && nx run query:version --type=path"
  }
}
